<!-- views/planites/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Planites Index</title>
  </head>
  <body>
    <h1>All Fruits</h1>
    <ul>
      <% planites.forEach(planiti => { %>
        <li><a href="/planites/<%= planiti._id %>"><%= planiti.name %></a></li>
      <% }); %>
    </ul>
   <button><a href="/planites/new">Add new Planet</a></button>
  </body>
</html>